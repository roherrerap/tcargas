<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <navigation-case>
            <from-outcome>main-admin</from-outcome>
            <to-view-id>/pages/administrator/mainadmin.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>main-operator</from-outcome>
            <to-view-id>/pages/operator/mainoperator.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>error-page</from-outcome>
            <to-view-id>/errorlogin.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>main-index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteSchedule</from-outcome>
            <to-view-id>/pages/operator/deleteSchedule.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>editSchedule</from-outcome>
            <to-view-id>/pages/operator/editSchedule.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>editSingleVehicles</from-outcome>
            <to-view-id>/pages/administrator/editSingleVehicles.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>editSingleTypeusers</from-outcome>
            <to-view-id>/pages/administrator/editsingleTypeusers.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>editSingleUsers</from-outcome>
            <to-view-id>/pages/administrator/editSingleUsers.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>listTypeusers</from-outcome>
            <to-view-id>/pages/administrator/listTypeusers.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>listVehicles</from-outcome>
            <to-view-id>/pages/administrator/listVehicles.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>listUsers</from-outcome>
            <to-view-id>/pages/administrator/listUsers.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>searchTypeusers</from-outcome>
            <to-view-id>/pages/administrator/resultTypeusers.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>searchVehicles</from-outcome>
            <to-view-id>/pages/administrator/resultVehicles.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>searchUsers</from-outcome>
            <to-view-id>/pages/administrator/resultUsers.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
